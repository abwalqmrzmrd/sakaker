<script>


document.addEventListener("DOMContentLoaded", function() {
  const icons = document.querySelectorAll('.icon-circle');
  let stopMovement = false; // ุญุงูุฉ ุชููู ุงูุญุฑูุฉ

  // ุงุฌุนู ูู ุฃููููุฉ ูุทููุฉ ุงููููุน ุจุญูุซ ุชุชุญุฑู ุจุญุฑูุฉ
  icons.forEach(icon => {
    icon.style.position = "absolute";
    icon.style.zIndex = 1; // ุฃููููุงุช ุฃุณุงุณูุฉ ุชุญุช ุงูุฃููููุงุช ุงููุฑุนูุฉ
  });

  function random(min, max) {
    return Math.random() * (max - min) + min;
  }

  function floatIcons() {
    if (!stopMovement) { // ุงูุญุฑูุฉ ููุท ุฅุฐุง ูู ุชุชููู
      const containerWidth = document.documentElement.scrollWidth;
      const containerHeight = document.documentElement.scrollHeight;

      icons.forEach(icon => {
        if (!icon.classList.contains('active')) {
          const x = random(0, containerWidth - icon.offsetWidth);
          const y = random(0, containerHeight - icon.offsetHeight);
          const rotation = random(-15, 15);
          const scale = random(0.9, 1.1);

          icon.style.top = y + "px";
          icon.style.left = x + "px";
          icon.style.transform = `rotate(${rotation}deg) scale(${scale})`;
        }
      });
    }
  }

  // ุจุฏุก ุงูุญุฑูุฉ ุงููุณุชูุฑุฉ
  floatIcons();
  setInterval(floatIcons, 3000);

  icons.forEach(icon => {
    icon.addEventListener('click', (e) => {
      e.stopPropagation(); // ููุน ุชูุนูู ุญุฏุซ document

      // ุชููู ุงูุญุฑูุฉ ุนูุฏ ูุชุญ ุงูุฃููููุฉ
      stopMovement = true;

      // ุชุญุฏูุฏ ุงูุฃููููุฉ ุงููุถุบูุทุฉ
      icons.forEach(i => i.classList.remove('active'));
      icon.classList.add('active');

      // ุฅุฎูุงุก ุฃู ููุชุจุงุช ูุฑุนูุฉ ููุชูุญุฉ
      document.querySelectorAll('.sub-icons').forEach(sub => {
        sub.style.display = 'none';
        sub.style.zIndex = 1; // ุฅุนุงุฏุฉ ุชุฑุชูุจ z-index ููุฃููููุงุช ุงููุฑุนูุฉ ุงููุฎููุฉ
      });

      // ุนุฑุถ ุงูุฃููููุงุช ุงููุฑุนูุฉ ููุฐู ุงูุฃููููุฉ
      let subIcons = icon.querySelector('.sub-icons');
      if (subIcons) {
        subIcons.style.display = 'block';
        const rect = icon.getBoundingClientRect();
        subIcons.style.position = 'fixed';
        subIcons.style.top = rect.bottom + 'px';
        subIcons.style.left = rect.left + 'px';
        subIcons.style.zIndex = 9999; // ุชุธู ููู ูู ุงูุฃููููุงุช ุงูุฃุฎุฑู
      }
    });
  });

  // ุนูุฏ ุงูุถุบุท ุฎุงุฑุฌ ุฃููููุฉ -> ุฅุฎูุงุก ุงููุฑุนูุฉ ูุงุณุชุฆูุงู ุงูุญุฑูุฉ
  document.addEventListener('click', () => {
    icons.forEach(i => i.classList.remove('active'));
    document.querySelectorAll('.sub-icons').forEach(sub => sub.style.display = 'none');
    stopMovement = false; // ุฅุนุงุฏุฉ ุงูุญุฑูุฉ
  });

  window.addEventListener('resize', floatIcons);
});
</script>
<style>
.icon-circle {
  transition: all 2s ease-in-out;
}
.sub-icons {
  transition: all 0.5s ease-in-out;
}
